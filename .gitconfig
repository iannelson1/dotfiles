[alias]
	ls = log --prety=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	lnc = log --pretty=format:"%h\\ %s\\ [%cn]"
	ci = commit
	filelog = log -u
	fl = log -u
	lasttag = describe --tags --abrev=0
	lt = describe --tags --abbrev=0
	st = status -s
	br = branch
	cl = clone
	co = checkout
	tc = shortlog -sn
	d = diff --color-words	
[user]
	name = Ian Nelson
	email = iannelson1@gmail.com
[core]
	editor = vim
	autocrlf = input
[color]
	ui = auto
	interactive = auto
